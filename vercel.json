{
  "version": 2,
  "name": "milo-newsletter-landing",
  "buildCommand": "cd react-version && npm run build",
  "outputDirectory": "react-version/build",
  "installCommand": "cd react-version && npm install",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}