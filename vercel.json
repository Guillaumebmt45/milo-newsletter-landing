{
  "version": 2,
  "name": "milo-newsletter-landing",
  "builds": [
    {
      "src": "react-version/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "react-version/build"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}